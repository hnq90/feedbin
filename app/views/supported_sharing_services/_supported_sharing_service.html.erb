<% if supported_sharing_service.active? %>
  <li><%= render partial: "supported_sharing_services/service_#{supported_sharing_service.service_id}", locals: {sharing_service: supported_sharing_service, record: supported_sharing_service, status: :active} %></li>
<% else %>
  <li><%= render partial: "supported_sharing_services/service_#{supported_sharing_service.service_id}", locals: {sharing_service: supported_sharing_service, record: supported_sharing_service, status: :issue} %></li>
<% end %>
