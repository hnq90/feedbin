<% if status == :active %>
  <%= render partial: "supported_sharing_services/delete_supported_service", locals: {sharing_service: sharing_service} %>
<% else %>
  <p class="service-title"><%= sharing_service[:label] %></p>
  <%= link_to 'Activate', supported_sharing_services_path(supported_sharing_service: {service_id: sharing_service[:service_id] }), class: 'list-action', method: :post %></small>
<% end %>